{
  "name": "FunBlocker",
  "version": "1.0",
  "description": "FunBlocker is a script to remove annoying posts in Facebook, mainly funny [re]posts.",
  "content_scripts": [
    {
        "matches": [
            "http://www.facebook.com/*",
            "https://www.facebook.com/*"
        ],
        "js": ["fb.js"]
    }
  ],
  "permissions": [
    "http://www.facebook.com/*",
    "https://www.facebook.com/*"
  ]
}